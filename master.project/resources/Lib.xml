<cookingActions>
	<CookingAction name="rollen">
		<Name>aufrollen</Name>
		<regs>
			<Regex result="first">.*~INGREDIENT.*~ACTION.*</Regex>
		</regs>
		<transformations>
			<Transformation>
				<AddIngredientTag>aufgerollt</AddIngredientTag>
			</Transformation>
		</transformations>
	</CookingAction>
	<CookingAction name="streichen">
		<Name>bestreichen</Name>
		<regs>
			<Regex result="first">.*~INGREDIENT.*mit.*~INGREDIENT.*~ACTION.*</Regex>
			<Regex result="last">.*~INGREDIENT.*auf.*~INGREDIENT.*~ACTION.*</Regex>
		</regs>
		<transformations>
			<Transformation>
				<AddIngredientTag>bestrichen</AddIngredientTag>
			</Transformation>
		</transformations>
	</CookingAction>
	<CookingAction name="klappen">
		<transformations>
			<Transformation>
				<AddIngredientTag>geklappt</AddIngredientTag>
			</Transformation>
		</transformations>
	</CookingAction>
	<CookingAction name="geben">
		<transformations>
			<Transformation>
				<AddIngredientTag>mit INGREDIENT</AddIngredientTag>
			</Transformation>
		</transformations>
	</CookingAction>
	<CookingAction name="schneiden">
		<regs>
			<Regex result="all">.*</Regex>
		</regs>
		<transformations>
			<Transformation>
				<AddIngredientTag>geschnitten</AddIngredientTag>
			</Transformation>
		</transformations>
	</CookingAction>
	<CookingAction name="drehen">
		<transformations>
			<Transformation>
				<AddIngredientTag>gedreht</AddIngredientTag>
			</Transformation>
		</transformations>
	</CookingAction>
	<CookingAction name="legen" />
	<CookingAction name="backen" />
	<CookingAction name="verteilen">
		<transformations>
			<Transformation>
				<AddIngredientTag>mit INGREDIENT</AddIngredientTag>
			</Transformation>
		</transformations>
	</CookingAction>
	<CookingAction name="verrühren">
		<transformations>
			<Transformation>
				<AddIngredientTag>verrührt</AddIngredientTag>
			</Transformation>
		</transformations>
	</CookingAction>
	<CookingAction name="einkochen">
		<transformations>
			<Transformation>
				<AddIngredientTag>eingekocht</AddIngredientTag>
			</Transformation>
		</transformations>
	</CookingAction>
	<CookingAction name="abschmecken">
		<transformations>
			<Transformation>
				<AddIngredientTag>mit INGREDIENT abgeschmeckt</AddIngredientTag>
			</Transformation>
		</transformations>
	</CookingAction>
	<CookingAction name="herstellen" />
	<CookingAction name="hochziehen">
		<transformations>
			<Transformation>
				<AddIngredientTag>hochgezogen</AddIngredientTag>
			</Transformation>
		</transformations>
	</CookingAction>
	<CookingAction name="untermischen">
		<transformations>
			<Transformation>
				<AddIngredientTag>mit INGREDIENT</AddIngredientTag>
			</Transformation>
		</transformations>
	</CookingAction>
</cookingActions>
